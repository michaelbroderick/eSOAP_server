        <div class="form-check mt-1 mr-4 float-right">
            <input class="form-check-input" type="checkbox" value="wcc" id="Hcheck1" name=haemolysedid>
            <label class="form-check-label" for="Hcheck1">Haemolysed</label>
        </div>

    </div>
    <div class="form-group col-sm-6">
